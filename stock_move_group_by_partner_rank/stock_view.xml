<?xml version="1.0"?>
<openerp>
  <data>
    <record id="view_move_search_partner_rank" model="ir.ui.view">
      <field name="name">stock.move.search.partner.rank</field>
      <field name="model">stock.move</field>
      <field name="inherit_id" ref="stock.view_move_search" />
      <field name="arch" type="xml">
        <xpath expr="//group[@string='Group By...']" position="inside">
          <filter string="Rank" domain="[]" context="{'group_by': 'rank_id'}" />
        </xpath>
      </field>
    </record>

  </data>
</openerp>

