<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

      <record id="act_delivered" model="workflow.activity">
        <field name="wkf_id" ref="stock.wkf_picking"/>
        <field name="name">delivered</field>
        <field name="kind">function</field>
        <field name="action">action_delivered()</field>
        <field name="flow_stop">True</field>
      </record>

      <record id="trans_done_delivered" model="workflow.transition">
        <field name="act_from" ref="stock.act_done"/>
        <field name="act_to" ref="act_delivered"/>
        <field name="signal">action_delivered</field>
      </record>


    </data>
</openerp>
